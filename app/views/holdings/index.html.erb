<div class="bs-docs-section">
  <div class="row">
    <div class="col-lg-12">
      <div class="well">
        <%= link_to 'Back', :back, :class => "btn btn-default" %>
        <h1>Holdings</h1>
        <table class="table table-striped table-hover ">
          <thead>
            <tr>
              <th>Loan id</th>
            	<th>Loan part id</th>
            </tr>
    		  </thead>
  		    <% @holdings.each do |holding| %>
            <tbody>
              <tr><td><%= holding["loan_id"]%></td></tr>
              <tr><td><%= holding["loan_part_id"] %></td></tr>
            </tbody>
          <% end %>
        </table>
      </div>
    </div>
  </div>
</div>